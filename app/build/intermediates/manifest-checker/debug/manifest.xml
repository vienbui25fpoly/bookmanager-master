<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tiennvph06776.bookmanager.project"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="27" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityStart"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityUser"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityStyleBook"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityBook"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityBill"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityBookTop10"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivtiyStatistics"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityHome"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tiennvph06776.bookmanager.project.activity.ActivityBillDetail"
            android:parentActivityName="com.tiennvph06776.bookmanager.project.activity.ActivityBill"
            android:screenOrientation="portrait" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.tiennvph06776.bookmanager.project.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>